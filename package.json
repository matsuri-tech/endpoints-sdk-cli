{
  "name": "endpoints-sdk-cli",
  "description": "endpoints sdk cli",
  "version": "1.0.0",
  "author": "hrdtbs",
  "bin": {
    "mes": "./bin/run"
  },
  "bugs": "https://github.com/matsuri-tech/endpoints-sdk-cli/issues",
  "dependencies": {
    "@oclif/color": "^0.1.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/prettier": "^1.19.0",
    "@types/supports-color": "^5.3.0",
    "chai": "^4",
    "cli-ux": "^5.4.1",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/matsuri-tech/endpoints-sdk-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mes",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "matsuri-tech/endpoints-sdk-cli",
  "scripts": {
    "dev": "tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "release": "yarn dev && yarn prepack && standard-version && git add README.md && git commit --amend --no-edit && git push --follow-tags origin master"
  },
  "types": "lib/index.d.ts"
}
