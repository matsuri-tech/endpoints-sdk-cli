{
  "name": "endpoints-sdk-cli",
  "description": "endpoints sdk cli",
  "version": "2.1.1",
  "author": "hrdtbs",
  "bin": {
    "mes": "./bin/run"
  },
  "bugs": "https://github.com/matsuri-tech/endpoints-sdk-cli/issues",
  "dependencies": {
    "@oclif/color": "^0.1.2",
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "cli-ux": "^5.4.9",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@types/prettier": "^2.0.2",
    "@types/rimraf": "^3.0.0",
    "@types/supports-color": "^5.3.0",
    "chai": "^4",
    "eslint": "^7.4.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^11.0.1",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "standard-version": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/matsuri-tech/endpoints-sdk-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mes",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "matsuri-tech/endpoints-sdk-cli",
  "scripts": {
    "dev": "tsc -b",
    "devlink": "yarn unlink && yarn dev && yarn link",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "release": "yarn dev && yarn prepack && standard-version && git add README.md && git commit --amend --no-edit && git push --follow-tags origin master"
  },
  "types": "lib/index.d.ts"
}
